  },
  "dependencies": {
    "@getalby/sdk": "^7.0.0",
    "bolt11": "^1.4.1",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "glob": "^10.3.10",
    "gray-matter": "^4.0.3",
    "satori": "^0.10.14",
    "sharp": "^0.33.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@netlify/functions": "^5.1.0"
  }
}
